
<%- include('partials/header'); -%>


   <%if(error){%>
   <p class="error"><%=error%></p>

   <%}%>
   

<form action="/Login" method="Post">
 
  <div class="regform" >
    <h2 class="h2Login">Sign In</h2>
    <a class="btn btn-block btn-social btn-google" href="/auth/google" role="button">
      <span class="fa fa-google"></span> Sign in with Google
    </a>
    <a class="btn btn-block btn-social btn-facebook">
      <span class="fa fa-facebook-f"></span> Sign in with Facebook
    </a>
    <p class=" btn-paragraphs">Or sign in manually </p>
  <fieldset >
   
  <div class="form-group">
    <div class="input-group-prepend">
      <div class="input-group-text"> <i class="fas fa-envelope fa-lg"></i></div>
    
    <input type="text" class="form-control" name="email" id="email" placeholder="Email" aria-describedby="emailHelp">
  </div>
  </div>
  
  <div class="form-group">
    <div class="input-group-prepend">
      <div class="input-group-text"><i class="fas fa-lock fa-lg"></i></div>
    
    <input type="password" class="form-control" name="password" id="passsword" placeholder="Password" aria-describedby="emailHelp">
  </div>
  </div>
   
  <button  type="submit" class="btnReg btn-lg fa-lg"  >Submit</button>
  <a href="/" class="btn btn-info ">Cancel</a>
  </fieldset>
 
</form>
  <a class="nav-link" href="/forgot">Forgottern Password?</a>
     
</div>





<%- include('partials/footer'); -%>





   



    
    
   


   




      


